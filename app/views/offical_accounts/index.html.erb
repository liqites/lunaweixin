<p id="notice"><%= notice %></p>

<h1>Listing Offical Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offical_accounts.each do |offical_account| %>
      <tr>
        <td><%= offical_account.name %></td>
        <td><%= link_to 'Show', offical_account %></td>
        <td><%= link_to 'Edit', edit_offical_account_path(offical_account) %></td>
        <td><%= link_to 'Destroy', offical_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Offical account', new_offical_account_path %>
