<div class="mui-tab-pane" id="pane-weixin">
<h1>微信公众号</h1>
  <table class="mui-table mui-table-bordered">
    <thead>
      <tr>
        <th>名称</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @offical_accounts.each do |offical_account| %>
        <tr>
          <td><%= offical_account.name %></td>
          <td><%= link_to '管理', offical_account %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <div class="mui-divider"></div>
  <%= link_to '新建', new_offical_account_path,class:'t-btn-ok'%>
</div>
