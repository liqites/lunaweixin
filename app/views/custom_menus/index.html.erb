<p id="notice"><%= notice %></p>

<h1>Listing Custom Menus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Func</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @custom_menus.each do |custom_menu| %>
      <tr>
        <td><%= custom_menu.name %></td>
        <td><%= custom_menu.func %></td>
        <td><%= link_to 'Show', custom_menu %></td>
        <td><%= link_to 'Edit', edit_custom_menu_path(custom_menu) %></td>
        <td><%= link_to 'Destroy', custom_menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Custom menu', new_custom_menu_path %>
